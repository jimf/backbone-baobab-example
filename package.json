{
  "name": "backbone-baobab",
  "version": "1.0.0",
  "description": "Simple app written with Backbone + Baobab",
  "browserify": {
    "transform": [
      "babelify",
      "hbsfy"
    ]
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build-js": "browserify app/src/main.js -o dist/js/app.js",
    "build-static": "cp app/index.html dist/index.html",
    "build": "npm run clean && mkdir -p dist/js && npm run build-static && npm run build-js",
    "start": "live-server dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "watchify app/src/main.js -o dist/js/app.js"
  },
  "author": "Jim Fitzpatrick",
  "license": "MIT",
  "devDependencies": {
    "babelify": "^6.1.0",
    "backbone": "^1.1.2",
    "baobab": "^1.1.0",
    "browserify": "^10.1.3",
    "handlebars": "^3.0.3",
    "hbsfy": "^2.2.1",
    "jquery": "^2.1.4",
    "live-server": "^0.7.1",
    "underscore": "^1.8.3",
    "watchify": "^3.2.1"
  }
}
